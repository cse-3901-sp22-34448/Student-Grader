<h1>Feedback Form</h1>

<%= form_with model: @feedback do |form| %>
  <div>
    <%= form.label :score %><br>
    <%= form.number_field :score, in: 1.0..10.0, step: 0.5 %>
  </div>
  <div>
    <%= form.label :comment %><br>
    <%= form.text_area :comment %>
  </div>
  <div>
    <%= form.label :event_id %><br>
    <%= form.collection_select :event_id, Event.order(:title), :id, :title %>
  </div>
    <%= form.hidden_field :student_id, :value => current_student.id %>
  <div>
    <%= form.submit %>
  </div>
<% end %>
