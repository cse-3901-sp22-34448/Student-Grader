<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<table>
  <tr>
    <th>Time</th>
    <th>Events</th>
  </tr>
<% @events.each do |event| %>
  <tr>
    <td>
    <%= event.time.strftime("%H:%M") %>
    </td>
    <td>
    <%= link_to event.title, event %>
    </td>
  </tr>
<% end %>
</table>